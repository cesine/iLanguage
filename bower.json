    "GATE",
    "Samples",
    "SampleData",
    "hmm",
    "libs",
    "CollocationsCollector",
    "Visualization",
    "LanguageTools",
    "MorphoParser",
    "build",
    "test-reports",
    "Gruntfile.js",
    "test",
    "libs",
    "src",
    "dist/main_bundle.js",
    "dist/test_bundle.js",
